<tr>
  <td class="whitespace-nowrap py-2 pl-4 pr-3 text-sm text-gray-500 sm:pl-0"><%= @audit_log_item.type %></td>
  <td class="whitespace-nowrap px-2 py-2 text-sm font-medium text-gray-900"><%= helpers.full_datetime(@audit_log_item.happened_at) %></td>
  <td class="px-2 py-2 text-sm text-gray-500"><%= @audit_log_item.thread_name %></td>
  <td class="px-2 py-2 text-sm text-gray-500"><%= @audit_log_item.thread_id %></td>
  <td class="px-2 py-2 text-sm text-gray-900"><%= @audit_log_item.previous_value %></td>
  <td class="px-2 py-2 text-sm text-gray-500"><%= @audit_log_item.new_value %></td>
</tr>
